<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:web="http://java.sun.com/xml/ns/javaee" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>Lab1</display-name>
  <welcome-file-list>
    <welcome-file>Search.jsp</welcome-file>
  </welcome-file-list>
  <description>
        SE Group Project.
    </description>
  <servlet>
    <servlet-name>SearchServlet</servlet-name>
    <servlet-class>TripPackage.Search</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SearchServlet</servlet-name>
    <url-pattern>/search</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>PlansServlet</servlet-name>
    <servlet-class>TripPackage.Plans</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PlansServlet</servlet-name>
    <url-pattern>/plans</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>detailsServlet</servlet-name>
    <servlet-class>TripPackage.Details</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>detailsServlet</servlet-name>
    <url-pattern>/details</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AdminLogin</servlet-name>
    <servlet-class>TripPackage.AdminManage</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AdminLogin</servlet-name>
    <url-pattern>/AdminLogin</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>AddLocationServlet</servlet-name>
    <servlet-class>TripPackage.AddLocationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddLocationServlet</servlet-name>
    <url-pattern>/addLocation</url-pattern>
  </servlet-mapping>
</web-app>